@page
@model IndexModel
@{
    ViewData["Title"] = "Home page  33";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <div class= "container border" style="padding:20px">
           <div class = "form-group row" >
               <div class = "col-2">
                   <label asp-for="Usuario"></label>
               </div>
               <div class = "col-5">
                   <input type="text" asp-for="Usuario" class="form-control"/>

               </div>
              
           </div>
           <div class = "form-group row" >
               <div class = "col-2">
                   <label asp-for="userlogged"></label>
               </div>
               <div class = "col-5">
                   <input type="password" asp-for="userlogged" class="form-control" />
               </div>
             
           </div>
           <div class = "form-group row" >
               <div class="col-5">
                    <span class="text-danger" >@HttpContext.Session.GetString("mensaje")</span>
                </div>
            </div>
    </div>

</div>
